{
    "name": "Youtube Downloader",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Download Youtube videos.",

    "content_scripts": [{
        "matches": ["https://www.youtube.com/watch*"],
        "js": [
            "main.js"
        ],
        "run_at":"document_end",
        "all_frames":true
    }],
    "permissions": [
        "tabs"
    ]
}